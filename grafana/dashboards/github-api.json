{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" },
    { "type": "panel", "id": "piechart", "name": "Pie chart", "version": "" }
  ],
  "id": null,
  "uid": "quickgithub-github-api",
  "title": "QuickGitHub - GitHub API",
  "description": "GitHub API call rates, rate limit hits, and latency distribution",
  "tags": ["quickgithub", "github", "otel"],
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 1,
  "time": { "from": "now-6h", "to": "now" },
  "refresh": "30s",
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0
      }
    ]
  },
  "panels": [
    {
      "title": "Total GitHub API Calls",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 0 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [{ "color": "blue", "value": null }]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(github_api_calls_total)",
          "legendFormat": "Total Calls",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "title": "Rate Limit Hits",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 0 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 10 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(github_api_rate_limit_hits_total)",
          "legendFormat": "Rate Limit Hits",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "title": "Avg API Latency",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 16, "y": 0 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1000 },
              { "color": "red", "value": 5000 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(github_api_latency_milliseconds_sum[5m])) / sum(rate(github_api_latency_milliseconds_count[5m]))",
          "legendFormat": "Avg Latency",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "title": "GitHub API Call Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 15
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "trees" },
            "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "files" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      },
      "targets": [
        {
          "expr": "sum by (github_api_endpoint) (rate(github_api_calls_total[5m]))",
          "legendFormat": "{{github_api_endpoint}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Rate Limit Hits Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80
          },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(increase(github_api_rate_limit_hits_total[5m]))",
          "legendFormat": "Rate Limit Hits",
          "refId": "A"
        }
      ]
    },
    {
      "title": "API Latency Percentiles",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "drawStyle": "line", "fillOpacity": 5 }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(github_api_latency_milliseconds_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(github_api_latency_milliseconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(github_api_latency_milliseconds_bucket[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    },
    {
      "title": "Calls by Endpoint",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "targets": [
        {
          "expr": "sum by (github_api_endpoint) (github_api_calls_total)",
          "legendFormat": "{{github_api_endpoint}}",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] },
        "pieType": "donut"
      }
    },
    {
      "title": "API Latency by HTTP Method",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 20 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, http_method) (rate(github_api_latency_milliseconds_bucket[5m])))",
          "legendFormat": "p95 - {{http_method}}",
          "refId": "A"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "version": 1
}
