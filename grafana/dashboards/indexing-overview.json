{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" },
    { "type": "panel", "id": "table", "name": "Table", "version": "" }
  ],
  "id": null,
  "uid": "quickgithub-indexing",
  "title": "QuickGitHub - Indexing Overview",
  "description": "Indexing pipeline success/failure rates, durations, and active jobs",
  "tags": ["quickgithub", "indexing", "otel"],
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 1,
  "time": { "from": "now-6h", "to": "now" },
  "refresh": "30s",
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0
      }
    ]
  },
  "panels": [
    {
      "title": "Total Indexing Jobs",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(indexing_total)",
          "legendFormat": "Total",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "title": "Successful Indexing Jobs",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(indexing_total{status=\"success\"})",
          "legendFormat": "Success",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "title": "Failed Indexing Jobs",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(indexing_errors_total)",
          "legendFormat": "Errors",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "title": "Avg Indexing Duration",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 60000 },
              { "color": "red", "value": 300000 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(indexing_duration_milliseconds_sum[5m])) / sum(rate(indexing_duration_milliseconds_count[5m]))",
          "legendFormat": "Avg Duration",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "title": "Indexing Success / Failure Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "stacking": { "mode": "normal" },
            "fillOpacity": 80
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Success" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Failed" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "targets": [
        {
          "expr": "sum(increase(indexing_total{status=\"success\"}[5m]))",
          "legendFormat": "Success",
          "refId": "A"
        },
        {
          "expr": "sum(increase(indexing_total{status=\"failed\"}[5m]))",
          "legendFormat": "Failed",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Indexing Duration Distribution",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(indexing_duration_milliseconds_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(indexing_duration_milliseconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(indexing_duration_milliseconds_bucket[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    },
    {
      "title": "Recent Indexing Jobs",
      "type": "table",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 12 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "targets": [
        {
          "expr": "sum by (repo_full_name, status) (increase(indexing_total[1h]))",
          "legendFormat": "{{repo_full_name}} - {{status}}",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "repo_full_name": "Repository",
              "status": "Status",
              "Value": "Count"
            }
          }
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "version": 1
}
