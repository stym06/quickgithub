{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" },
    { "type": "panel", "id": "piechart", "name": "Pie chart", "version": "" },
    { "type": "panel", "id": "table", "name": "Table", "version": "" },
    { "type": "panel", "id": "row", "name": "Row", "version": "" }
  ],
  "id": null,
  "uid": "quickgithub-worker",
  "title": "QuickGitHub - Worker Overview",
  "description": "Unified operational dashboard for the Python worker: indexing pipeline, LLM usage, costs, and per-repo breakdown",
  "tags": ["quickgithub", "worker", "otel"],
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 1,
  "time": { "from": "now-6h", "to": "now" },
  "refresh": "30s",
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0
      }
    ]
  },
  "panels": [
    {
      "title": "Indexing Pipeline",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "title": "Total Jobs",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [{ "color": "blue", "value": null }]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(indexing_total)",
          "legendFormat": "Total",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "title": "Success Rate",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.8 },
              { "color": "green", "value": 0.95 }
            ]
          },
          "min": 0,
          "max": 1
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(indexing_total{status=\"success\"}) / sum(indexing_total)",
          "legendFormat": "Success Rate",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "title": "Errors",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(indexing_errors_total)",
          "legendFormat": "Errors",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "title": "Avg Duration",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 120000 },
              { "color": "red", "value": 300000 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(indexing_duration_milliseconds_sum[5m])) / sum(rate(indexing_duration_milliseconds_count[5m]))",
          "legendFormat": "Avg",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "title": "Total LLM Cost",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 10 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(llm_cost_usd_total)",
          "legendFormat": "Cost",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "title": "LLM Calls",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [{ "color": "purple", "value": null }]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(llm_calls_total)",
          "legendFormat": "Calls",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "title": "Indexing Throughput",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 5 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "stacking": { "mode": "normal" },
            "fillOpacity": 80
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Success" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Failed" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "targets": [
        {
          "expr": "sum(increase(indexing_total{status=\"success\"}[5m]))",
          "legendFormat": "Success",
          "refId": "A"
        },
        {
          "expr": "sum(increase(indexing_total{status=\"failed\"}[5m]))",
          "legendFormat": "Failed",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Indexing Duration Percentiles",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 5 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(indexing_duration_milliseconds_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(indexing_duration_milliseconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(indexing_duration_milliseconds_bucket[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    },
    {
      "title": "LLM Usage",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "collapsed": false
    },
    {
      "title": "Token Usage Rate (Input vs Output)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 15,
            "stacking": { "mode": "normal" }
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": "Input.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": "Output.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }]
          }
        ]
      },
      "targets": [
        {
          "expr": "sum by (gen_ai_request_model) (increase(gen_ai_usage_input_tokens_total[5m]))",
          "legendFormat": "Input - {{gen_ai_request_model}}",
          "refId": "A"
        },
        {
          "expr": "sum by (gen_ai_request_model) (increase(gen_ai_usage_output_tokens_total[5m]))",
          "legendFormat": "Output - {{gen_ai_request_model}}",
          "refId": "B"
        }
      ]
    },
    {
      "title": "LLM Call Latency Percentiles",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "drawStyle": "line", "fillOpacity": 5 }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(llm_latency_milliseconds_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(llm_latency_milliseconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(llm_latency_milliseconds_bucket[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    },
    {
      "title": "Costs & Efficiency",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "collapsed": false
    },
    {
      "title": "LLM Cost Over Time (by Model)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 23 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "stacking": { "mode": "normal" }
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum by (gen_ai_request_model) (increase(llm_cost_usd_total[5m]))",
          "legendFormat": "{{gen_ai_request_model}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Avg Cost per Indexing Job",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 23 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "custom": { "drawStyle": "line", "fillOpacity": 10 },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 2 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "increase(llm_cost_usd_total[1h]) / clamp_min(increase(indexing_total[1h]), 1)",
          "legendFormat": "Cost per Job",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Breakdown",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 31 },
      "collapsed": false
    },
    {
      "title": "LLM Calls by Model",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 32 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "targets": [
        {
          "expr": "sum by (gen_ai_request_model) (llm_calls_total)",
          "legendFormat": "{{gen_ai_request_model}}",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "values": ["value", "percent"] },
        "pieType": "donut"
      }
    },
    {
      "title": "Token Split (Input vs Output)",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 32 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "targets": [
        {
          "expr": "sum(gen_ai_usage_input_tokens_total)",
          "legendFormat": "Input Tokens",
          "refId": "A",
          "instant": true
        },
        {
          "expr": "sum(gen_ai_usage_output_tokens_total)",
          "legendFormat": "Output Tokens",
          "refId": "B",
          "instant": true
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "values": ["value", "percent"] },
        "pieType": "donut"
      }
    },
    {
      "title": "LLM Latency by Model (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 32 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le, gen_ai_request_model) (rate(llm_latency_milliseconds_bucket[5m])))",
          "legendFormat": "p50 - {{gen_ai_request_model}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le, gen_ai_request_model) (rate(llm_latency_milliseconds_bucket[5m])))",
          "legendFormat": "p95 - {{gen_ai_request_model}}",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Recent Indexing Jobs by Repo",
      "type": "table",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 40 },
      "datasource": { "uid": "${DS_PROMETHEUS}", "type": "prometheus" },
      "targets": [
        {
          "expr": "sum by (repo_full_name, status) (increase(indexing_total[24h]))",
          "legendFormat": "{{repo_full_name}} - {{status}}",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true },
            "renameByName": {
              "repo_full_name": "Repository",
              "status": "Status",
              "Value": "Jobs (24h)"
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Status" },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-text"
                }
              },
              {
                "id": "mappings",
                "value": [
                  { "type": "value", "options": { "success": { "text": "Success", "color": "green" } } },
                  { "type": "value", "options": { "failed": { "text": "Failed", "color": "red" } } }
                ]
              }
            ]
          }
        ]
      }
    }
  ],
  "schemaVersion": 39,
  "version": 1
}
